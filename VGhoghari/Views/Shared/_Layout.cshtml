<!DOCTYPE HTML>
<html class="no-js">
<head>
  <!-- Basic Page Needs
  ================================================== -->
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <title>@ViewBag.Title - VGhoghari.org : Shree Visha Shrimali Ghoghari Mahila Federation</title>
  <meta name="description" content="">
  <meta name="keywords" content="">
  <meta name="author" content="Rajan Sanghvi">

  <!-- Mobile Specific Metas
  ================================================== -->
  <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">
  <meta name="format-detection" content="telephone=no">

  <!-- CSS
  ================================================== -->
  @Styles.Render("~/Theme/css/bootstrap.css")
  @Styles.Render("~/Theme/plugins/mediaelement/mediaelementplayer.css")
  @Styles.Render("~/Theme/css/style.css")
  @Styles.Render("~/Theme/plugins/prettyphoto/css/prettyPhoto.css")

  @RenderSection("css", required: false)

  <!--[if lte IE 8]><link rel="stylesheet" type="text/css" href=" @Url.Content("~/Theme/css/ie8.css")" media="screen" /><![endif]-->
  <!-- Color Style -->
  @Styles.Render("~/Theme/colors/color1.css")
  @Styles.Render("~/Theme/css/custom.css")

  <!-- SCRIPTS
  ================================================== -->
  @Scripts.Render("~/Theme/js/modernizr.js")<!-- Modernizr -->
</head>
<body>
  <!--[if lt IE 7]>
    <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
  <![endif]-->

  <div class="body">
    <div class="se-pre-con"></div>
    <!-- Start Site Header -->
    <header class="site-header">
      <div class="topbar">
        <div class="container">
          <div class="row">
            <div class="col-md-4 col-sm-6 col-xs-8">
              <h1 class="logo"> <a href="@Url.Action("Index", "Home")"><img src="@Url.Content("~/Theme/images/logo.png")" alt="Logo"></a> </h1>
            </div>
            <div class="col-md-8 col-sm-6 col-xs-4">
              <ul class="top-navigation hidden-sm hidden-xs">
                @if (User.Identity.IsAuthenticated) {
                  <li>@Html.ActionLink("My Profile", "MyProfile", "User")</li>
                  <li>@Html.ActionLink("Logout", "Logout", "User")</li>
                }
                else {
                  <li>@Html.ActionLink("Register", "Register", "User")</li>
                  <li>@Html.ActionLink("Login", "Login", "User")</li>
                }
                <li>@Html.ActionLink("Donate", "Donate", "Home")</li>
              </ul>
              <a href="#" class="visible-sm visible-xs menu-toggle"><i class="fa fa-bars"></i></a>
            </div>
          </div>
        </div>
      </div>
      <div class="main-menu-wrapper">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <nav class="navigation">
                <ul class="sf-menu">
                  <li>
                    @Html.ActionLink("Home", "Index", "Home")
                  </li>
                  <li>
                    @Html.ActionLink("About Us", "AboutUs", "Home")
                    <ul class="dropdown">
                      <li>@Html.ActionLink("Overview", "AboutUs", "Home")</li>
                      <li>@Html.ActionLink("Contact Us", "ContactUs", "Home")</li>
                    </ul>
                  </li>
                  <li>
                    @Html.ActionLink("Matrimonial", "AllBiodata", "Matrimonial")
                    <ul class="dropdown">
                      <li>@Html.ActionLink("View All Biodata", "AllBiodata", "Matrimonial")</li>
                      <li>@Html.ActionLink("Manage Existing Biodata", "Manage", "Matrimonial")</li>
                      <li>@Html.ActionLink("Add New Biodata", "AddBasicInfo", "Matrimonial")</li>
                    </ul>
                  </li>
                  <li>
                    @Html.ActionLink("Events", "List", "Event")
                    <ul class="dropdown">
                      <li>@Html.ActionLink("Upcoming Events", "List", "Event", new { eventType = "upcoming" }, null)</li>
                      <li>@Html.ActionLink("Ongoing Events", "List", "Event", new { eventType = "ongoing" }, null)</li>
                      <li>@Html.ActionLink("Previous Events", "List", "Event", new { eventType = "previous" }, null)</li>
                    </ul>
                  </li>
                  <li>
                    @Html.ActionLink("Porjects", "List", "Project")
                  </li>
                  <li>
                    @Html.ActionLink("Forms", "Forms", "Home")
                  </li>
                  <li>
                    @Html.ActionLink("Gallery", "Gallery", "Home")
                  </li>
                  <li>
                    @Html.ActionLink("Contact Us", "ContactUs", "Home")
                  </li>
                </ul>
              </nav>
            </div>
          </div>
        </div>
      </div>
    </header>
    <!-- End Site Header -->
    @RenderBody()
    <!-- Start Footer -->
    
    <footer class="site-footer-bottom">
      <div class="container">
        <div class="row">
          <div class="copyrights-col-left col-md-6 col-sm-6">
            <p>&copy; @DateTime.Now.Year VGhoghari.org : Shree Visha Shrimal Ghoghari Mahila Federation. All Rights Reserved</p>
          </div>
          <div class="copyrights-col-right col-md-6 col-sm-6">
            <div class="social-icons"> <a href="https://www.facebook.com/" target="_blank"><i class="fa fa-facebook"></i></a> <a href="https://twitter.com/" target="_blank"><i class="fa fa-twitter"></i></a> <a href="http://www.pinterest.com/" target="_blank"><i class="fa fa-pinterest"></i></a> <a href="https://plus.google.com/" target="_blank"><i class="fa fa-google-plus"></i></a> <a href="http://www.pinterest.com/" target="_blank"><i class="fa fa-youtube"></i></a> <a href="#"><i class="fa fa-rss"></i></a> </div>
          </div>
        </div>
      </div>
    </footer>
    <!-- End Footer -->
    <a id="back-to-top"><i class="fa fa-angle-double-up"></i></a>
  </div>

  @Scripts.Render("~/Theme/js/jquery-2.0.0.min.js")<!-- Jquery Library Call -->
  @Scripts.Render("~/Theme/plugins/prettyphoto/js/prettyphoto.js")<!-- PrettyPhoto Plugin -->
  @Scripts.Render("~/Theme/js/helper-plugins.js")<!-- Plugins -->
  @Scripts.Render("~/Theme/js/bootstrap.js")<!-- UI -->
  @Scripts.Render("~/Theme/js/waypoints.js")<!-- Waypoints -->
  @Scripts.Render("~/Theme/plugins/mediaelement/mediaelement-and-player.min.js")<!-- MediaElements -->
  @Scripts.Render("~/Theme/js/init.js")<!-- All Scripts -->
  @Scripts.Render("~/Theme/plugins/flexslider/js/jquery.flexslider.js")<!-- FlexSlider -->
  @Scripts.Render("~/Theme/plugins/countdown/js/jquery.countdown.min.js")<!-- Jquery Timer -->

  @RenderSection("scripts", false)

  <script>
    $(window).load(function () {
      // Animate loader off screen
      $(".se-pre-con").fadeOut("slow");;
    });

    $('.se-pre-con').bind('ajaxStart', function () {
      $(this).show();
    }).bind('ajaxStop', function () {
      $(this).hide();
    });
  </script>
</body>
</html>